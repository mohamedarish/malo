{
    "manifest_version": 2,
    "name": "malo",
    "version": "1.0",
    "description": "malayalam grammarly definitions for manglish",

    "icons": {
        "48": "icons/42.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./dist/content.js"]
        }
    ],

    "background": {
        "page": "background.html"
    },
    "permissions": ["<all_urls>", "menus", "activeTab"],
    "web_accessible_resources": [
        "./dist/dictionary/fetchData.js",
        "./dist/convertor/convert.js"
    ]
}
