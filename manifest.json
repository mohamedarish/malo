{
    "manifest_version": 2,
    "name": "malo",
    "version": "1.0",
    "description": "malayalam grammarly definitions for manglish",

    "icons": {
        "48": "icons/42.png"
    },

    "content_scripts": {
        "matches": ["*://*.*.*/*", "*://*.*/*"],
        "js": [".dist/scripts/viewPopup.js"]
    },

    "background": {
        "page": "background.html"
    },
    "permissions": ["menus", "activeTab"]
}
