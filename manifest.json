{
    "manifest_version": 2,
    "name": "malo",
    "version": "1.0",
    "description": "malayalam grammarly definitions for manglish",

    "icons": {
        "48": "icons/42.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./dist/content_scripts/index.js"]
        }
    ],
    "background": {
        "scripts": ["./dist/background_scripts/index.js"]
    },
    "permissions": ["menus", "activeTab", "tabs"]
}
